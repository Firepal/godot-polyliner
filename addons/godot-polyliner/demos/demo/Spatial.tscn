script/source = "@tool
extends Node3D

@export var mul = 1.0
@export var xmul = 1.0

var timer = 0.0
var xtimer = 0.0
func _process(delta):
	timer += delta*mul
	xtimer += delta*xmul
#	rotation.x = sin(timer)*PI
	rotation.y = fmod(xtimer,TAU)
	position.x = sin(timer)*0.4
#	rotation.y = sin(timer)*1.5+1.2
"

